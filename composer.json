{
	"name": "oliverthiele/ot-irrebuttons",
	"type": "typo3-cms-extension",
	"description": "Adds buttons to content elements with IRRE",
	"homepage": "https://typo3.org",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Oliver Thiele",
			"email": "mail@oliver-thiele.de",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=8.1",
		"typo3/cms-core": "^12.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"conflict": {
	},
	"autoload": {
		"psr-4": {
			"OliverThiele\\OtIrrebuttons\\": "Classes/"
